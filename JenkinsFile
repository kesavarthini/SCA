properties([
  buildDiscarder(logRotator(numToKeepStr: '3')),
  disableConcurrentBuilds(),
])
pipeline {
    agent any
	  stages {
        stage('Build Docker '){
            steps{
                echo 'Building docker image'
                   sh ' sudo docker build -t image_of_order_service -f order-service/Dockerfile'
				 	//sh 'sudo mvn -f DataService/pom.xml clean install pmd:pmd sonar:sonar'
				 	//sh 'sudo mvn -f order-service/pom.xml clean install pmd:pmd sonar:sonar'
                }
        }
        stage('Docker Run') {
                steps {
                    sh 'sudo docker run  order_service'
                }
            }
	   stage('Test') {
                steps {
                    echo 'This is the Test Stage'
                }
		
            }
        }

}


